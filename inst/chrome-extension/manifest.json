{
	"manifest_version": 2,
	"name": "SearchRecorder",
	"description": "A Chrome extension for recording bibliographic database search histories",
	"version": "0.0.0.9",
	"browser_action": {
		"default_icon": "img/128_icon.png",
		"default_popup": "index.html",
		"default_title": "Click to start typing..."
	},
	"permissions": ["clipboardWrite", "clipboardRead", "downloads"],
	"icons": {
		"128": "img/128_icon.png",
		"48": "img/48_icon.png",
		"16": "img/16_icon.png"
	}
}
